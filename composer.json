{
    "name": "Eurovision-vote-counter",
    "description": "A program to count votes for the Eurovision song contest under a redesigned voting system.",
    "type": "skeleton",
    "require": {
        "php": "^8.2",
        "julien-boudry/condorcet": "dev-dev-4.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2",
        "laravel/pint": "^1.10"
    },
    "scripts": {
        "test": "phpunit",
        "pintify": "pint"
    },
    "autoload": {
        "psr-4": {
            "EurovisionVoting\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EurovisionVoting\\Tests\\": "tests/"
        }
    }
}
